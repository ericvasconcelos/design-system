{
  "name": "@ericvasconcelos/design-system",
  "version": "3.0.5",
  "description": "Biblioteca com os componentes em React para o frontend",
  "main": "dist/bundle.js",
  "module": "dist/bundle.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "badges": "jest-coverage-badges output './badges'",
    "build-storybook": "build-storybook",
    "build": "rollup -c",
    "coverage": "jest --coverage",
    "commit": "./bin/commit.sh",
    "gitmoji-init": "gitmoji -i",
    "lint:css": "git diff --diff-filter=d --cached --name-only -z -- '*.js' '*.jsx' | xargs -0 -I % sh -c 'git show \":%\" | stylelint \"%\";'",
    "lint:js": "git diff --diff-filter=d --cached --name-only -z -- '*.js' '*.jsx' | xargs -0 -I % sh -c 'git show \":%\" | eslint --stdin --stdin-filename \"%\";'",
    "lint-all:css": "stylelint './src/**/*.js'",
    "lint-all:js": "eslint src --ext .js,.jsx",
    "make": "./bin/make.sh",
    "prepublishOnly": "rollup -c",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "CI=true jest --modulePaths=src --no-cache --runInBand --bail",
    "test:staged": "cross-env CI=true jest --bail --coverage --modulePaths=src --runInBand",
    "test-all": "jest --watchAll --modulePaths=src --coverage",
    "testDebug": "node --inspect-brk ./node_modules/.bin/jest --watch  --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ericvasconcelos/design-system.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericvasconcelos/design-system/issues"
  },
  "homepage": "https://github.com/ericvasconcelos/design-system#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.10",
    "@storybook/addon-a11y": "6.4",
    "@storybook/addon-actions": "^6.2",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.2",
    "@storybook/addon-links": "^6.2",
    "@storybook/addons": "^6.2",
    "@storybook/react": "^6.2",
    "@storybook/theming": "^6.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "file-loader": "^6.2.0",
    "formik": "^2.2.6",
    "gitmoji-cli": "^3.3.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-matcher-utils": "^26.6.2",
    "jest-next-dynamic": "^1.0.1",
    "jest-styled-components": "^7.0.3",
    "jsdom": "^16.4.0",
    "lint-staged": ">=10",
    "next": "^10.0.6",
    "prettier": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.38.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-uglify": "^6.0.4",
    "styled-components": "^5.2.1",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.2.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "uglify-js": "^3.12.6",
    "vanilla-masker": "^1.2.0",
    "yup": "^0.32.8"
  },
  "peerDependencies": {
    "formik": ">= 2.x.x",
    "next": ">= 10.x.x",
    "prop-types": ">= 15.x.x",
    "react": ">= 17.x.x",
    "react-dom": ">= 17.x.x",
    "styled-components": ">= 5.x.x",
    "vanilla-masker": ">= 1.x.x"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:css",
      "yarn lint:js",
      "yarn test:staged"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "react-popper": "^2.2.5"
  }
}
